
<%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
<div class="container">
<p>
<strong>Gacetas:</strong>
</p>
<ul>
<% @documents.each do |document| %>
<li>
  <%= document.name %>
  <%= link_to "Ediar", edit_document_path(document) %>
  <%= link_to "Ver", document %>
  <%= link_to 'Borrar', document, method: :delete, data: { confirm: 'Seguro?' } %>
</li>
<% end %>
</ul>
</div>
