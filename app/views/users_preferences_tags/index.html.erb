<p id="notice"><%= notice %></p>

<h1>Users Preferences Tags</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_preferences_tags.each do |users_preferences_tag| %>
      <tr>
        <td><%= users_preferences_tag.tag_id %></td>
        <td><%= link_to 'Show', users_preferences_tag %></td>
        <td><%= link_to 'Edit', edit_users_preferences_tag_path(users_preferences_tag) %></td>
        <td><%= link_to 'Destroy', users_preferences_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users Preferences Tag', new_users_preferences_tag_path %>
